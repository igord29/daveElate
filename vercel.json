{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "ANAM_API_KEY": "@anam-api-key",
    "OPENAI_API_KEY": "@openai-api-key",
    "DAVE_MODEL": "gpt4o-mini",
    "NODE_ENV": "production"
  },
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
}
